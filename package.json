{
  "name": "pft-svelte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "concurrently \"cd ./POCKETBASE/ && pocketbase serve\" \"vite dev\"",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "pb:start": "cd ./POCKETBASE/ && pocketbase --dev serve"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-netlify": "^2.0.8",
    "@sveltejs/kit": "^1.30.4",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.41.0",
    "postcss": "^8.4.39",
    "postcss-load-config": "^4.0.2",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "5.0.0-next.171",
    "svelte-check": "^3.8.4",
    "svelte-preprocess": "^5.1.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0",
    "vite": "^4.5.3"
  },
  "type": "module",
  "dependencies": {
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "pocketbase": "^0.21.3"
  },
  "packageManager": "pnpm@8.6.7+sha1.019f4b93c5b07fb12e3097bae3d8f2d0cff444b7"
}
